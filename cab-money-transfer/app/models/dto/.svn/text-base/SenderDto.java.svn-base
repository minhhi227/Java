package models.dto;

import java.io.Serializable;
import java.util.Date;

/**
 * 
 * @author Chamnan
 *
 */
public class SenderDto implements Serializable{
    

	/**
     * serialVersionUID
     */
    private static final long serialVersionUID = 1L;
   // sender.id, sender.fullName, sender.companyName, sender.identityNumber, sender.accountNo
    private long _id;
    private String _fullName;
    private String _companyName;
    private String _identityNumber;
    private String _accountNo;
    private String _fullNameIdentitiCard;
    private Date _dateOfBirth;
    private Date _expireDate;
    private String _address;
    private String _type;
    
    public SenderDto(){
        super();
    }

    public SenderDto(long _id, String _fullName, String _companyName, String _identityNumber, String _accountNo, String _fullNameIdentitiCard, Date _dateOfBirth, Date _expireDate, String _address, String type) {
		super();
		this._id = _id;
		this._fullName = _fullName;
		this._companyName = _companyName;
		this._identityNumber = _identityNumber;
		this._accountNo = _accountNo;
		this._fullNameIdentitiCard = _fullNameIdentitiCard;
		this._dateOfBirth = _dateOfBirth;
		this._expireDate = _expireDate;
		this._address = _address;
		this._type = type;
	}

    
	public long getId() {
		return _id;
	}

	public void setId(final long id) {
		this._id = id;
	}

	public String getFullName() {
		return _fullName;
	}

	public void setFullName(final String fullName) {
		this._fullName = fullName;
	}

	public String getCompanyName() {
		return _companyName;
	}

	public void setCompanyName(final String companyName) {
		this._companyName = companyName;
	}

	public String getIdentityNumber() {
		return _identityNumber;
	}

	public void setIdentityNumber(final String identityNumber) {
		this._identityNumber = identityNumber;
	}

	public String getAccountNo() {
		return _accountNo;
	}

	public void setAccountNo(final String accountNo) {
		this._accountNo = accountNo;
	}

	public String getFullNameIdentitiCard() {
		return _fullNameIdentitiCard;
	}

	public void setFullNameIdentitiCard(final String fullNameIdentitiCard) {
		this._fullNameIdentitiCard = fullNameIdentitiCard;
	}

	public Date getDateOfBirth() {
		return _dateOfBirth;
	}

	public void setDateOfBirth(final Date dateOfBirth) {
		this._dateOfBirth = dateOfBirth;
	}

	public Date getExpireDate() {
		return _expireDate;
	}

	public void setExpireDate(final Date expireDate) {
		this._expireDate = expireDate;
	}

	public String getAddress() {
		return _address;
	}

	public void setAddress(final String address) {
		this._address = address;
	}

	public String getType() {
		return _type;
	}

	public void setType(String type) {
		this._type = type;
	}
    
    
    
}
